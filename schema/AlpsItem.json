{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "AlpsItem.json",
  "type": "object",
  "examples": [
    {
      "$ref": "../example/AlpsItem.json"
    }
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "isPublic": {
      "type": "boolean"
    },
    "title": {
      "type": "string",
      "examples": [
        "MaAlps"
      ]
    },
    "user": {
      "$ref": "./User.json"
    },
    "mediaType": {
      "type": "string",
      "enum": [
        "application/alps+json",
        "application/alps+xml"
      ]
    }
  }
}
